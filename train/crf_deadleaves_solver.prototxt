net: "crf_deadleaves.prototxt"

solver_type: 0
#rms_decay: 0.2

display: 20

lr_policy: "fixed"
base_lr: 1e-8
momentum: 0.9

weight_decay: 0.0005

# The maximum number of iterations, DO NOT INCREASE THIS FURTHER
max_iter: 20000

# snapshot intermediate results
snapshot: 5000
snapshot_prefix: "crf_deadleaves" 

# solver mode: CPU or GPU
solver_mode: CPU

