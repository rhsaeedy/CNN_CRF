net: "crf_new.prototxt"

solver_type: 0
#rms_decay: 0.2

display: 20

lr_policy: "fixed"
base_lr: 1e-16
momentum: 0.9

weight_decay: 0.0005

# The maximum number of iterations, DO NOT INCREASE THIS FURTHER
max_iter: 10000

# snapshot intermediate results
snapshot: 2000
snapshot_prefix: "crf_new" 

# solver mode: CPU or GPU
solver_mode: CPU

